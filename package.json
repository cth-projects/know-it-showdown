{
  "name": "know-it-showdown",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "setup": "bun install && bun db:migrate && bun db:seed",
    "test:worldbank": "bun scripts/test-worldbank.ts",
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "prisma migrate dev",
